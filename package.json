{
	"name": "cargo-land",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "env-cmd -f .env vite --port 9000",
		"build": "env-cmd -f .env tsc && vite build",
		"start": "env-cmd -f .env vite --port 9000 --host 0.0.0.0",
		"preview": "vite preview",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "eslint",
		"lint2": "npx eslint \"src/**/*.{js,jsx,ts,tsx,html}\" --quiet --fix",
		"format:check": "prettier --check .",
		"format:write": "prettier --write ."
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"rules": {
			"react-hooks/exhaustive-deps": [
				"warn",
				{}
			]
		}
	},
	"dependencies": {
		"@react-google-maps/api": "^2.18.1",
		"@react-oauth/google": "^0.9.0",
		"@types/googlemaps": "^3.43.3",
		"@types/leaflet-routing-machine": "^3.2.4",
		"@types/react-geocode": "^0.2.1",
		"@types/react-places-autocomplete": "^7.2.9",
		"animate.css": "^4.1.1",
		"aos": "^2.3.4",
		"autoprefixer": "^10.4.14",
		"axios": "1.3.5",
		"chart.js": "^4.2.1",
		"country-state-city": "^3.1.2",
		"env-cmd": "^10.1.0",
		"eslint": "8.38.0",
		"eslint-config-react-app": "^7.0.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"flutterwave-react-v3": "^1.3.0",
		"framer-motion": "10.11.6",
		"hero-slider": "^3.1.0",
		"jwt-decode": "^3.1.2",
		"react": "^18.2.0",
		"react-chartjs-2": "^5.2.0",
		"react-confirm-alert": "^3.0.6",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.43.9",
